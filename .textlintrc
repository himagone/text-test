{
    "rules": {
        "preset-jtf-style": {
            "3.1.1.全角文字と半角文字の間": false,
        }
        "preset-ja-technical-writing": {
          "no-mix-dearu-desumasu": {
            "preferInHeader": "",
            "preferInBody": "ですます",
            "preferInList": "である",
            "strict": false
          },
          "ja-no-weak-phrase": true,
          "no-unmatched-pair": true,
          "no-doubled-conjunction": true,
          "ja-no-abusage": true
        }
    }
}
